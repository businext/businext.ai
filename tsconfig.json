{
	"compilerOptions": {
		"lib": ["ES2017", "esnext.asynciterable"],
		"module": "commonjs",
		"noImplicitReturns": true,
		"outDir": "lib",
		"sourceMap": true,
		"target": "es2017",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"strictPropertyInitialization": true,
		"strictNullChecks": true,
		"skipLibCheck": true,
		"resolveJsonModule": true
	},
	"compileOnSave": true,
	"include": ["src"]
}
